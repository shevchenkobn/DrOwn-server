{
  "name": "drown-server",
  "version": "0.1.0",
  "description": "Backend for DrOwn course project",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "npm:g:dev": "sudo npm i rimraf tslint typescript nodemon tslint-config-airbnb -g -D",
    "npm:g:dev:win": "npm i rimraf tslint typescript nodemon tslint-config-airbnb -g -D",
    "build": "rimraf ./dist && tsc",
    "start": "node ./dist",
    "start:brk": "node --inspect-brk ./dist",
    "start:b:brk": "npm run build && node --inspect-brk ./dist",
    "start:dev": "node --inspect=9229 ./dist",
    "start:b:dev": "npm run build && node --inspect=9229 ./dist",
    "watch": "nodemon -w src/**/*.* -x \"npm run start:dev\"",
    "db:clear": "node dist/console/drop-sql-tables",
    "db:add": "node dist/console/add-sql-tables"
  },
  "engines": {
    "node": "^10.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shevchenkobn/DrOwn-server.git"
  },
  "author": "Bogdan Shevchenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shevchenkobn/DrOwn-server/issues"
  },
  "homepage": "https://github.com/shevchenkobn/DrOwn-server#readme",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "brcypt": "^1.0.1",
    "config": "^2.0.1",
    "dataloader": "^1.4.0",
    "express": "^4.16.4",
    "globby": "^8.0.1",
    "inversify": "^5.0.1",
    "js-yaml": "^3.12.0",
    "json-schema-ref-parser": "^6.0.2",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "mysql": "^2.16.0",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.12",
    "swagger-tools": "^0.10.4",
    "tslib": "^1.9.3",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.0",
    "@types/globby": "^8.0.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/knex": "^0.15.1",
    "@types/mysql2": "github:types/mysql2",
    "@types/node": "^10.12.9",
    "@types/reflect-metadata": "^0.1.0",
    "@types/yargs": "^12.0.1",
    "tslib": "^1.9.3"
  },
  "optionalDependencies": {}
}
